{
  "compilerOptions": {
   
    "strictNullChecks": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "target": "ES6",
    "lib": ["ES2017", "DOM"],
    "skipLibCheck": true,
    "types": ["node"]
  },
  "exclude": ["node_modules","lib","es"]
}
